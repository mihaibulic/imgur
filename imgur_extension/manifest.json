{
    "manifest_version": 2,

    "name": "Imgur Fullscreen Gallery",
    "description": "This extension makes imgur better (try it with the Hover Zoom extension). ",
    "version": "1.1",
    "icons": 
        {
            "16": "icon16.png",
            "48": "icon48.png",
            "128":"icon128.png" 
        },
    "content_scripts": [{
        "matches": ["http://imgur.com/"],
        "js": ["imgur.js"]
    }],
    "permissions" : [ "storage" ]
}
